{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://the-internet.herokuapp.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Dynamic Loading"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Example 2: Element rendered after the fact"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "button"
      }
    },
    {
      "type": "waitForElementPresent",
      "locator": {
        "type": "id",
        "value": "finish"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "Hello World!"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}