{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://the-internet.herokuapp.com/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Dropdown"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "css selector",
        "value": "#dropdown option[value='1']"
      }
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "id",
        "value": "dropdown"
      },
      "value": "1"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}